<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @project.name %>
</p>

<p>
  <b>Description:</b>
  <%= @project.description %>
</p>

<p>
  <b>Worked on:</b>
  <%= @project.worked_on %>
</p>

<p>
  <b>Category:</b>
  <%= @project.category_id %>
</p>

<% for image in @project.images %>
<p>
	<img src="<%= image.image.url %>" /><br/>
	<%= image.title %>
</p>
<% end %>
<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
